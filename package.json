{
  "name": "akihabara-retro-game-shop-be",
  "version": "1.0.0",
  "description": "This is the back-end api for the akihabara retro game shop front end",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "mup": "knex migrate:up",
    "mdown": "knex migrate:down",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migrateh": "heroku run knex migrate:latest -a akihabara-retro-game-shop-be",
    "rollbackh": "heroku run knex migrate:rollback -a akihabara-retro-game-shop-be",
    "databaseh": "heroku pg:psql -a akihabara-retro-game-shop-be",
    "seedh": "heroku run knex seed:run -a akihabara-retro-game-shop-be",
    "deploy": "git push heroku main",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand"
  },
  "author": "Stephen Aranda",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "knex": "^3.1.0",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.4"
  }
}